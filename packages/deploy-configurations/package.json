{
  "name": "@oasisdex/deploy-configurations",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint:fix": "eslint . --fix",
    "lint": "eslint . --max-warnings=0",
    "tsc": "tsc",
    "watch": "tsc -w",
    "build": "tsc -b -v && tsc-alias -p tsconfig.json"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./addresses": {
        "import": "./lib/addresses/index.js",
        "types": "./lib/addresses/index.d.ts"
    },
    "./configs": {
        "import": "./lib/configs/index.js",
        "types": "./lib/configs/index.d.ts"
    },
    "./constants": {
        "import": "./lib/constants/index.js",
        "types": "./lib/constants/index.d.ts"
    },
    "./operation-definitions": {
        "import": "./lib/operation-definitions/index.js",
        "types": "./lib/operation-definitions/index.d.ts"
    },
    "./types": {
        "import": "./lib/types/index.js",
        "types": "./lib/types/index.d.ts"
    },
    "./utils": {
        "import": "./lib/utils/index.js",
        "types": "./lib/utils/index.d.ts"
    }
  },
  "dependencies": {
    "@safe-global/safe-core-sdk": "^3.3.2",
    "@safe-global/safe-core-sdk-types": "^1.9.0",
    "@safe-global/safe-ethers-adapters": "^0.1.0-alpha.16",
    "@safe-global/safe-ethers-lib": "^1.9.2",
    "@safe-global/safe-service-client": "^2.0.0",
    "ethers": "^5.7.2",
    "node-cache": "^5.1.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@oasisdex/typescript-config": "workspace:*",
    "@oasisdex/eslint-config": "workspace:*",
    "eslint": "^8.56.0",
    "tsc-alias": "^1.8.8"
  }
}
