{
  "name": "@oasisdex/dma-library",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.min.js",
  "typings": "lib/esm/index.d.ts",
  "version": "0.2.16",
  "scripts": {
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:esm": "yarn tsc -p tsconfig.esm.json && webpack",
    "build:cjs": "yarn tsc -p tsconfig.cjs.json && yarn tscpaths -p tsconfig.cjs.json -s src -o lib/cjs",
    "clean": "rm -rf ./lib",
    "watch": "yarn tsc -b -w",
    "lint:fix": "yarn eslint . --fix --ext .ts",
    "lint": "yarn eslint . --ext .ts --max-warnings=0 && yarn tsc -p tsconfig.json --noEmit",
    "prepublish": "yarn build"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@oasisdex/abis": "*",
    "@oasisdex/addresses": "*",
    "@oasisdex/dma-common": "*",
    "@oasisdex/dma-deployments": "*",
    "@oasisdex/domain": "*",
    "bignumber.js": "^9.0.1",
    "ethers": "5.7.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mocha": "^10.1.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "stream-browserify": "^3.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "ts-loader": "^9.4.2",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "tscpaths": "^0.0.9",
    "typescript": "^5.0.4",
    "vm-browserify": "^1.1.2",
    "web-streams-polyfill": "^3.2.1",
    "webpack": "^5.79.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1"
  }
}
