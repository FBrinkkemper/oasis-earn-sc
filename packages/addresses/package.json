{
  "name": "@oasisdex/addresses",
  "version": "1.0.0",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.min.js",
  "typings": "lib/esm/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:esm": "yarn tsc -p tsconfig.esm.json && webpack",
    "build:cjs": "yarn tsc -p tsconfig.cjs.json",
    "clean": "rm -rf ./lib",
    "watch": "yarn tsc -b -w",
    "lint": "yarn tsc -p tsconfig.base.json --noEmit",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "@oasisdex/dma-deployments": "*"
  }
}
