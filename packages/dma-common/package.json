{
  "name": "@oasisdex/dma-common",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -f tsconfig.tsbuildinfo && find ./ -name \"*.d.ts\" -delete && find ./ -name \"*.d.ts.map\" -delete ",
    "lint:fix": "eslint . --fix",
    "lint": "eslint . --max-warnings=0",
    "tsc": "tsc",
    "watch": "tsc -w",
    "build": "tsc -b -v"
  },
  "exports": {
    "./constants": {
      "import": "./lib/constants/index.js",
      "types": "./lib/constants/index.d.ts"
    },
    "./constants/*": {
      "import": "./lib/constants/*.js",
      "types": "./lib/constants/*.d.ts"
    },
    "./test-utils": {
      "import": "./lib/operation-definitions/index.js",
      "types": "./lib/operation-definitions/index.d.ts"
    },
    "./test-utils/*": {
      "import": "./lib/operation-definitions/*.js",
      "types": "./lib/operation-definitions/*.d.ts"
    },
    "./types": {
      "import": "./lib/types/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "./types/*": {
      "import": "./lib/types/*.js",
      "types": "./lib/types/*.d.ts"
    },
    "./utils": {
      "import": "./lib/utils/index.js",
      "types": "./lib/utils/index.d.ts"
    },
    "./utils/*": {
      "import": "./lib/utils/*/index.js",
      "types": "./lib/utils/*/index.d.ts"
    }
  },
  "dependencies": {
    "@ethersproject/providers": "^5.4.7",
    "axios": "^1.3.6",
    "bignumber.js": "9.0.1",
    "ethers": "5.7.2",
    "ts-try": "^0.2.0",
    "@oasisdex/deploy-configurations": "workspace:*",
    "@oasisdex/abis": "workspace:*",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "hardhat": "^2.13.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@oasisdex/eslint-config": "workspace:*",
    "@oasisdex/typescript-config": "workspace:*",
    "@types/sinon-chai": "^3.2.9",
    "eslint": "^8.56.0"
  }
}
