{
  "author": "Summer.di devs",
  "bin": {
    "summer-cli": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^4",
    "conf-cli": "^0.1.9",
    "ethers": "^6.8.1"
  },
  "description": "A cli that will bring us one step closer to nirvana. 🙏",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.53.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^10",
    "oclif": "^3.17.2",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/OasisDEX/oasis-earn-sc/tree/dev/packages/summer-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@oasisdex/cli",
  "oclif": {
    "bin": "summer-cli",
    "dirname": "summer-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "conf-cli"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Configure cli to automaticly use your wallet, network etc."
      }
    }
  },
  "repository": "https://github.com/OasisDEX/oasis-earn-sc",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/codeberg-studio/summer-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "nx": {
    "implicitDependencies": ["@oasisdex/deploy-configurations"]
  }
}
